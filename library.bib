@techreport{Kwon2020,
abstract = {The gender gap in the labor market is still salient, even in the most developed countries. This paper documents how the expansion of high-speed rail (HSR) in South Korea contributes to the decrease in the gender gap in labor market outcomes. With its high level of gender disparity , South Korea provides an ideal setting to study this issue, thanks to extensive geocoded micro panel datasets and the massive expansion of HSR beginning in 2004. Using fixed effects estimators, I empirically show that the gender gap in labor market decreases with the expansion of HSR. The results are also robust to an IV strategy, leveraging historical railroads constructed in the Japanese colonial era. Guided by a spatial general equilibrium model, I show that HSR increases the labor demand in local service industries such as retail, education, medical , and restaurant sectors which more intensively hire women. Also, women in non-core areas (i.e. outside of the Seoul metropolitan areas) benefit from the improvement in local amenities, particularly in the area of education, thereby reducing their childcare burden and encouraging them into the labor force.},
author = {Kwon, Eunjee},
booktitle = {Working Paper},
