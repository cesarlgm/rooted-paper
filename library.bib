@article{Markus1986,
abstract = {The concept of possible selves is introduced to complement current conceptions of self-knowledge. Possible selves represent individuals' ideas of what they might become, what they would like to become, and what they are afraid of becoming, and thus provide a conceptual link between cognition and motivation. Possible selves are the cognitive components of hopes, fears, goals, and threats, and they give the specific self-relevant form, meaning, organization, and direction to these dynamics. Possible selves are important, first, because they function as incentives for future behavior (i.e., they are selves to be approached or avoided) and second, because they provide an evaluative and interpretive context for the current view of self. A discussion of the nature and function of possible selves is followed by an exploration of their role in addressing several persistent problems, including the stability and malleability of the self, the unity of the self, self-distortion, and the relationship between the self-concept and behavior. {\textcopyright} 1986 American Psychological Association.},
author = {Markus, Hazel and Nurius, Paula},
