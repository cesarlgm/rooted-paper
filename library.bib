@article{Freund2023,
abstract = {This paper proposes a model of the firm as a "team assembly technology," linking the evolution of firm-level inequality to shifts in the nature of production. In the model, firms assign tasks to workers who vary in general quality and task-specific skills; hiring takes place in a frictional labor market. Worker-task specialization makes team production more advantageous but also endoge-nously generates coworker complementarities, so that the least capable team member's quality disproportionately influences joint output. In equilibrium, employers therefore hire workers of similar quality, and firms with superstar teams pull away in terms of productivity and pay. The model's key mechanisms are validated using rich administrative micro data. A theory-informed measure of coworker complementarities doubles from 1990 to 2010, mirroring a shift towards greater task complexity. A structural estimation exercise suggests that this rise explains close to 40% of the empirically observed rise in the between-firm share of wage inequality in Germany. Additionally, the model provides insights on how the interaction between specialization and labor markets influences total factor productivity.},
author = {Freund, Lukas},
doi = {10.2139/ssrn.4312245},
file = {:C\:/Users/thecs/Dropbox/0_central_library/Papers/Freund - 2023 - Superstar Teams The Micro Origins and Macro Implications of Coworker Complementarities - SSRN Electronic Journal.pdf:pdf},
journal = {SSRN Electronic Journal},
keywords = {firms,inequality,matching,productivity,specialization,teams *},
title = {{Superstar Teams: The Micro Origins and Macro Implications of Coworker Complementarities}},
url = {www.lukasfreund.com.},
year = {2023}
}
