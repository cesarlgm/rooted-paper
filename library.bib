}
@article{Gollin2021,
abstract = {In most developing countries, wages are substantially higher in cities than in rural areas. One possible explanation is that the higher wage levels of urban areas are offset by lower non-monetary amenities. This paper draws on new high-resolution evidence to document how non-monetary amenities vary between urban and rural areas within 20 Sub-Saharan African countries. We focus on measures of public goods, crime and pollution. We find that in almost all countries, and for almost all measures, the quality of amenities is at least as high in cities as in rural areas. This finding casts doubt on the hypothesis that urban wage premia in the developing world represent a compensation for lower amenities.},
