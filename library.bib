file = {:C\:/Users/thecs/Dropbox/0_central_library/Papers//Card et al. - 2012 - Inequality at work The effect of peer salaries on job satisfaction - American Economic Review.pdf:pdf;:C\:/Users/thecs/Dropbox/0_central_library/Papers//Card et al. - 2012 - Inequality at work The effect of peer salaries on job satisfaction - American Economic Review.pdf:pdf;:C\:/Users/thecs/Dropbox/0_central_library/Papers/Card et al. - 2012 - Inequality at work The effect of peer salaries on job satisfaction - American Economic Review.pdf:pdf},
issn = {00028282},
journal = {American Economic Review},
month = {oct},
