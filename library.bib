isbn = {9780444634597},
issn = {15740692},
keywords = {Cost effectiveness,Demand for education,Education in developing countries,Learning outcomes,Pedagogy,Policy evaluation,School enrollment and attendance,School governance,School inputs},
month = {jan},
pages = {653--743},
publisher = {Elsevier},
title = {{Improving Education Outcomes in Developing Countries: Evidence, Knowledge Gaps, and Policy Implications}},
volume = {5},
