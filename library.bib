journal = {American Economic Review},
number = {2},
pages = {948--980},
title = {{Selection, Agriculture, and Cross-Country Productivity Differences}},
url = {http://dx.doi.org/10.1257/aer.103.2.948http://dx.doi.org/10.1257/aer.103.2.948.},
